<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Plants & Flowers -->
        <record id="symbol_seedling" model="greenhouse.symbol.library">
            <field name="name">Seedling</field>
            <field name="symbol">🌱</field>
            <field name="category">plants</field>
            <field name="description">Young plant, new growth, starting fresh</field>
            <field name="is_seasonal" eval="True"/>
            <field name="season_type">spring</field>
        </record>
        
        <record id="symbol_evergreen" model="greenhouse.symbol.library">
            <field name="name">Evergreen Tree</field>
            <field name="symbol">🌲</field>
            <field name="category">plants</field>
